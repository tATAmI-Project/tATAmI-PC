(agent Odd
	(behavior
		(initial register
			(if (readK (struct knowledge pattern ?Ana -isa-> ?what))
			then
			(send parent (struct message ?Ana -isa-> ?what))
			)
		)
		(reactive increase
			(receive (message counting ?number))
			(increment ?number ?newNo)
			(send parent (struct message counting ?newNo))
		)
	)
)
